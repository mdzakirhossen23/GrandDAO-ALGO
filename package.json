{
  "name": "GrandDAO",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "generate-client": "algokitgen generate -a /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/artifacts/Greendao.json -o /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/clients/GreendaoClient.ts",
    "compile-contract": "tealscript /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/greendao.algo.ts /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/artifacts",
    "generate-components": "algokit-generate-component /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/artifacts/Greendao.json /workspace/GrandDAO-ALGO/contracts/algo-contract/contracts/artifacts/components",
    "algo-build": "npm run compile-contract && npm run generate-client",
    "build": "npm run css:build && next build",
    "start": "next start",
    "lint": "next lint",
    "css:watch": "npx tailwindcss -i ./styles/globals.css -o ./public/output.css -w",
    "css:build": "npx tailwindcss -i ./styles/globals.css -o ./public/output.css",
    "deploy": "cd contracts/solidity && hardhat deploy --network moonbase",
    "verify": "cd contracts/solidity && hardhat verify --network moonbase $(cat address.txt)",
    "deploy-verify": "npm run deploy && npm run verify"
  },
